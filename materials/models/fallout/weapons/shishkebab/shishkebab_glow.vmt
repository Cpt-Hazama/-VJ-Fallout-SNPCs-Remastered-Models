//"UnlitTwoTexture_DX6"
"VertexLitGeneric"
{
 	"$model" 1
    "$nocull" "1"
	"$basetexture" "models/oblivion/flameatronach/flame"
	"$texture2" "models/oblivion/flameatronach/flame"

"$translucent" 1

	// -- Not supported by VertexLitGeneric ?
	"Proxies"
	{
		"TextureScroll"
		{
			"texturescrollvar" "$basetexturetransform"
			"texturescrollrate" 0.6
			"texturescrollangle" 75
		}
		"AnimatedTexture"
		{
			"animatedTextureVar" "$basetexture"
			"animatedTextureFrameNumVar" "$frame"
			"animatedTextureFrameRate" "16"
		}
	}
}